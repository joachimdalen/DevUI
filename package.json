{
  "name": "@joachimdalen/devui",
  "version": "1.2.0",
  "description": "",
  "main": "build/main.js",
  "types": "build/index.d.ts",
  "files": [
    "build/**/*"
  ],
  "scripts": {
    "storybook": "start-storybook -p 6006 -c .storybook",
    "build-components": "webpack -p --progress --colors --display-error-details",
    "build-css": "node-sass src/styles/main.scss build/main.css",
    "build-storybook": "build-storybook -s public",
    "build": "npm run build-components && npm run build-css",
    "test": "./node_modules/.bin/jest",
    "lint:styles": "npx stylelint 'src/**/**.scss'",
    "lint:ts": "eslint src/**/*.tsx",
    "lint:styles:fix": "npx stylelint 'src/**/**.scss' --fix",
    "lint:ts:fix": "eslint src/**/*.tsx --fix",
    "lint": "npm run lint:styles && npm run lint:ts",
    "autoformat": "./node_modules/.bin/prettier --write 'src/**/**.scss' 'src/**/*.tsx'",
    "lintfmt": "npm run lint:styles:fix && npm run lint:ts:fix && npm run autoformat"
  },
  "keywords": [
    "react",
    "components",
    "ui-library"
  ],
  "author": "Joachim Dalen <joachim@dalenapps.no> (https://joachimdalen.no)",
  "license": "GPL-3.0",
  "repository": "github:joachimdalen/DevUI",
  "bugs": {
    "url": "https://github.com/joachimdalen/DevUI/issues"
  },
